<ul>
    <% sessions.map((session, index)=>{
        %><li>
            <h3>
                <%=session.title%>
            </h3>
            <p>
                <%=session.description.slice(0,300)%>
            </p>
            <a href="/sessions/<%=session._id%>"> Learn More</a>
        </li>
        <%})%>
</ul>